<resources xmlns:tools="http://schemas.android.com/tools">

    <!-- Base application theme. -->
    <style name="TestAppTheme" parent="CommonTheme">
        <!--//自定义属性实现换主题，如果与夜间模式并存，则需要在夜间模式文件夹中存在-->
        <item name="pageBg">@color/colorBg</item>
    </style>

    <!--另一套主题-->
    <style name="TestAppTheme.Theme_exp_1">
        <item name="colorPrimary">#2196F3</item>
        <item name="colorPrimaryVariant">#1976D2</item>
        <item name="colorOnPrimary">@color/black</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">#0097A7</item>
        <item name="colorSecondaryVariant">#00BCD4</item>
        <item name="colorOnSecondary">@color/black</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor" tools:targetApi="l">?attr/colorPrimaryVariant</item>

        <item name="pageBg">@color/colorBg2</item>

    </style>

</resources>