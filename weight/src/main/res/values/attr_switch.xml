<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="Switch">
        <attr name="switch_direction">
            <flag name="left2Right" value="0" />
            <flag name="right2Left" value="1" />
        </attr>
        <attr name="switch_checkFrom">
            <flag name="any" value="0" />
            <flag name="click" value="1" />
            <flag name="call" value="2" />
        </attr>
        <attr name="switch_checkPadding" format="dimension" />
        <attr name="switch_speed" format="float" />
        <attr name="android:checked" />
        <attr name="switch_checkBgColor" format="color" />
        <attr name="switch_checkCenterColor" format="color" />
        <attr name="switch_uncheckBgColor" format="color" />
        <attr name="switch_uncheckCenterColor" format="color" />
    </declare-styleable>
</resources>