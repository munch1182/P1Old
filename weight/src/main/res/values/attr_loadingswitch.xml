<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="LoadingSwitch">
        <attr name="switch_direction" />
        <attr name="switch_checkFrom" />
        <attr name="switch_checkPadding" />
        <attr name="switch_speed" />
        <attr name="android:checked" />
        <attr name="switch_checkBgColor" />
        <attr name="switch_checkCenterColor" />
        <attr name="switch_uncheckBgColor" />
        <attr name="switch_uncheckCenterColor" />

        <attr name="loadingSwitch_initializeState">
            <flag name="loading" value="1" />
            <flag name="check" value="0" />
        </attr>
    </declare-styleable>
</resources>